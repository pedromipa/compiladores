:DEC
parametro:INT
fatorial:INT

:PROG 
# Calcula o fatorial de um número inteiro
LER parametro
fatorial := parametro 
SE parametro == 0 ENTAO fatorial := 1 #hehehehe hehehe 
ENQTO parametro > 1
INI
fatorial := fatorial * (parametro _ 1) 
parametro := parametro - 1
FIM
IMPRIMIR fatorial
IMPRIMIR "Oi"
